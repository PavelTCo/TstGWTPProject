<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'
	xmlns:npui="urn:import:com.gwtplatform.client.core">
<ui:with type="com.gwtplatform.client.place.NameTokens" field="nameTokens"></ui:with>
	<g:HTMLPanel styleName="wrap">
		<g:VerticalPanel width="461px" height="109px">
			<g:Hyperlink targetHistoryToken="{nameTokens.getHomepage}" styleName="logo"/>
			<g:VerticalPanel styleName="topbar" width="100%" >
				<npui:MainMenuView width="100%" />
			</g:VerticalPanel>
			
		</g:VerticalPanel>
        
    <g:FlowPanel ui:field="mainContentPanel" />
        
    <npui:MainMenuView />
  </g:HTMLPanel>

</ui:UiBinder>
